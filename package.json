{
  "name": "monorepo-base",
  "private": true,
  "license": "MIT",
  "engines": {
    "yarn": ">=1.15.2"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "postinstall": "yarn build",
    "format": "wsrun -c format",
    "format:fix": "wsrun -c format:fix",
    "lint": "wsrun -c lint",
    "lint:fix": "wsrun -c lint:fix",
    "typecheck": "wsrun -c typecheck",
    "build": "tsc --build --verbose && wsrun -m -c post-build",
    "watch": "tsc --build --verbose --watch",
    "clean": "wsrun -c clean",
    "compile-contracts": "./scripts/compile-contracts/compile-contracts.sh && wsrun -m -c compile-contracts",
    "test": "yarn compile-contracts && wsrun -mse -c test",
    "test:fix": "yarn lint:fix && yarn test && yarn format:fix && yarn typecheck",
    "no-git-changes": "./scripts/no-git-changes.sh",
    "check-examples": "./scripts/check-examples.sh",
    "prerelease": "yarn lint && yarn format && yarn test && yarn typecheck",
    "release": "yarn build && yarn changeset publish"
  },
  "dependencies": {
    "@aave/protocol-v2": "^1.0.1",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.11.2",
    "@nomiclabs/hardhat-ethers": "^2.0.1",
    "@nomiclabs/hardhat-etherscan": "^2.1.1",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@types/chai": "4.2.11",
    "@types/lowdb": "1.0.9",
    "@types/mocha": "^8.2.1",
    "@types/node": "14.0.5",
    "@typechain/ethers-v5": "^6.0.2",
    "@typechain/truffle-v5": "^4.0.1",
    "@typechain/web3-v1": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "4.15.1",
    "@typescript-eslint/parser": "4.15.1",
    "bignumber.js": "9.0.0",
    "chai": "^4.3.3",
    "chai-bignumber": "3.0.0",
    "chai-bn": "^0.2.1",
    "dotenv": "^8.2.0",
    "earljs": "^0.1.0",
    "eslint": "^6.8.0",
    "eslint-config-typestrict": "^1.0.0",
    "eslint-plugin-no-only-tests": "^2.4.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.0.31",
    "fp-ts": "^2.9.5",
    "hardhat": "^2.1.1",
    "hardhat-deploy": "^0.7.0-beta.46",
    "hardhat-gas-reporter": "^1.0.4",
    "hardhat-typechain": "^0.3.5",
    "io-ts": "^2.2.15",
    "mocha": "^8.2.0",
    "prettier": "^2.2.1",
    "prettier-plugin-solidity": "^1.0.0-beta.5",
    "solc": "^0.6.4",
    "solidity-coverage": "^0.7.16",
    "ts-node": "9.1.1",
    "typechain": "^4.0.3",
    "typescript": "^4.2.3",
    "wsrun": "^5.2.4"
  }
}
